{
  "name": "lambda-edge-api",
  "description": "Deploy your API to the edge using AWS-CDK",
  "version": "0.2.17",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/lambda-edge-api/issues",
  "homepage": "https://github.com/jill64/lambda-edge-api#readme",
  "files": [
    "dist",
    "bin",
    "cdk.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "aws",
    "aws-cdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/lambda-edge-api.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "build:demo": "cd demo && pnpm build",
    "deploy:demo": "cd demo && npm run deploy",
    "prepack": "npm run build",
    "test": "cd demo && pnpm test",
    "format": "npx prettier --write .",
    "lint": "npx prettier --check . && npx eslint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5"
  },
  "dependencies": {
    "@types/node": "20.8.5",
    "aws-cdk": "2.101.0",
    "aws-cdk-lib": "2.101.0",
    "constructs": "10.3.0",
    "esbuild": "0.19.4",
    "typescript": "5.2.2"
  }
}
