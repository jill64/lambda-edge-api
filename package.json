{
  "name": "lambda-edge-api",
  "description": "üåê Deploy your API to the edge using AWS-CDK",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/index.js",
  "workspaces": [
    "demo"
  ],
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/lambda-edge-api/issues",
  "homepage": "https://github.com/jill64/lambda-edge-api#readme",
  "files": [
    "dist",
    "bin",
    "cdk.json",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "api",
    "aws",
    "cdk",
    "edge",
    "lambda"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/lambda-edge-api.git",
    "image": "https://opengraph.githubassets.com/641f8a361d84c727154285eb6d71fadf63255cc5f7db4c4319841a4cdd23c16c/jill64/lambda-edge-api"
  },
  "scripts": {
    "build": "tsc && bunx publint",
    "build:demo": "cd demo && bun run build",
    "deploy:demo": "cd demo && bun run deploy",
    "prepack": "bun run build",
    "format": "bunx prettier -w .",
    "lint": "bun run lint:lib && bun run lint:demo",
    "lint:lib": "eslint . && bunx depcheck",
    "lint:demo": "cd demo && bun run lint"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "2.0.5",
    "@jill64/prettier-config": "1.0.0",
    "@types/node": "22.10.10",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.177.0",
    "constructs": "10.4.2",
    "esbuild": "0.25.0"
  }
}
