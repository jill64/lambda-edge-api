{
  "name": "lambda-edge-api",
  "description": "üåê Deploy your API to the edge using AWS-CDK",
  "version": "1.1.2",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/lambda-edge-api/issues",
  "homepage": "https://github.com/jill64/lambda-edge-api#readme",
  "files": [
    "dist",
    "bin",
    "cdk.json",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "api",
    "aws",
    "cdk",
    "edge",
    "lambda"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/lambda-edge-api.git"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "build:demo": "cd demo && pnpm build",
    "deploy:demo": "cd demo && npm run deploy",
    "prepack": "pnpm build",
    "format": "npx prettier -w .",
    "lint": "pnpm lint:lib && pnpm lint:demo",
    "lint:lib": "npx eslint . && npx depcheck",
    "lint:demo": "cd demo && pnpm lint"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "^0.1.5",
    "@jill64/prettier-config": "^0.0.2",
    "@types/node": "20.10.1",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.111.0",
    "constructs": "10.3.0",
    "esbuild": "0.19.8"
  }
}
